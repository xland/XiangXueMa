{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "runSvelte": "rollup -c ./script/renderer.config.js -w",
    "buildMain": "rollup --config rollup.main.config.js",
    "runElectron": "electron ./public/main.js",
    "watchPublic": "sirv public --no-clear",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start1": "concurrently \"npm run runSvelte\" \"npm run runElectron\"",
    "prestart": "rollup --config ./script/main.config.js",
    "start": "rollup -c ./script/renderer.config.js -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "concurrently": "^7.0.0",
    "electron": "^18.0.1",
    "electron-rebuild": "^3.2.7",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.23.13",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.49.11",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^7.5.1",
    "knex": "^1.0.4"
  }
}
